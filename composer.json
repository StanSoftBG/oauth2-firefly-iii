{
    "name" : "stansoft/oauth2-firefly-iii",
    "description": "Firefly-III OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "keywords": [
        "oauth",
        "oauth2",
        "authorization",
        "authentication",
        "idp",
        "identity",
        "sso",
        "single sign on",
        "firefly"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Stanimir Stoyanov",
            "email": "ss@stansoft.bg"
        }
    ],
    "require": {
        "php" : ">=5.6.0",
        "league/oauth2-client": ">=1.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "StanSoft\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StanSoft\\OAuth2\\Client\\Test\\": "test/src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    }
}
